{
  "name": "plugin-math-games",
  "version": "0.2.6",
  "description": "This plugin displays math games. The games can be displayed until a button response is given, or for a fixed length of time.",
  "type": "module",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "typings": "dist/index.d.ts",
  "unpkg": "dist/index.browser.min.js",
  "files": [
    "src",
    "dist"
  ],
  "source": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "tsc": "tsc",
    "build": "rollup --config",
    "build:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jchu10/plugin-math-games.git",
    "directory": "./"
  },
  "keywords": [
    "jsPsych"
  ],
  "author": {
    "name": "Junyi Chu",
    "url": "https://github.com/jchu10"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jchu10/plugin-math-games/issues"
  },
  "homepage": "/plugin-math-games/README.md",
  "dependencies": {
    "@citation-js/core": "^0.7.14",
    "@citation-js/plugin-bibtex": "^0.7.14",
    "@citation-js/plugin-cff": "^0.6.1",
    "@citation-js/plugin-software-formats": "^0.6.1",
    "commonjs": "^0.0.1",
    "jspsych": "^8.2.2",
    "phaser": "^3.90.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "terser": "^5.44.1"
  },
  "devDependencies": {
    "@jspsych/config": "^3.2.2",
    "@jspsych/test-utils": "^1.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-url": "^8.0.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "phaser3spectorjs": "^0.0.8",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-jest": "^29.4.5",
    "typescript": "~5.7.2",
    "vite": "^6.3.1"
  }
}
